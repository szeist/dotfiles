Config {
    font = "xft:Dejavu Sans Mono:pixelsize=12",
    bgColor = "#101010",
    fgColor = "#d0d0d0",
    position = TopW L 95,
    commands = [
        Run Cpu ["-L", "3", "-H","50", "--normal", "green", "--high", "red"] 10,
        Run Memory ["-t", "Mem: <usedratio>%"] 10,
        Run Date "%a %b %_d %l:%M" "date" 60,
        Run Wireless "wlo1" ["-L", "40", "-H", "70", "--normal", "yellow", "--high", "green", "--low", "red", "-t", "<essid> (<quality>%)"] 60,
        Run Network "enp0s25" [] 10,
        Run BatteryP ["BAT0"] ["-t", "<acstatus>", "-L", "10", "-H", "80", "-l", "red", "-h", "green", "--", "-O", "Charging", "-o", "Batt: <left>%" ] 120,
        Run Volume "default" "Master" [] 10,
        Run StdinReader
    ],
    sepChar = "%"
    alignSep = "}{"
    template = "%StdinReader% }{ %cpu% %memory% | %battery% | %enp0s25% | %wlo1wi% | %default:Master% | <fc=#ee9a00>%date%</fc> |"
}
